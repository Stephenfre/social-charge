{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "ios": {
        "buildType": "app-bundle"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true",
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
